<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.mc.service.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>GetPic</servlet-name>
    <servlet-class>com.mc.service.GetPic</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ServiceMain</servlet-name>
    <servlet-class>com.mc.service.ServiceMain</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>SelfServiceChangePW</servlet-name>
    <servlet-class>com.mc.service.SelfServiceChangePW</servlet-class>
  </servlet>




  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/LoginServlet.jsp</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GetPic</servlet-name>
    <url-pattern>/GetPic.jsp</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ServiceMain</servlet-name>
    <url-pattern>/ServiceMain.jsp</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SelfServiceChangePW</servlet-name>
    <url-pattern>/SelfServiceChangePW</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
